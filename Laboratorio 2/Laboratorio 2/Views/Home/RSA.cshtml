
@{
	ViewBag.Title = "RSA";
}

<h2>RSA</h2>
<br />
<h2 class="danger">@ViewBag.Error</h2>
<h2 class="success">@ViewBag.Ok</h2>

<form method="post" enctype="multipart/form-data">
	<h4>Ingrese Archivo</h4>
	<input type="file" name="ArchivoEntrada" />
	<br />
	<h4>Ingrese Llave</h4>
	<input type="file" name="ArchivoLlave" />
	<br />
	<button type="submit">Iniciar</button>
</form>